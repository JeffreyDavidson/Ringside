{
    "name": "jeffreydavidson/ringside",
    "type": "project",
    "description": "A project to help wrestling promoters run their companies.",
    "keywords": [
        "laravel",
        "wrestling"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "ankurk91/laravel-eloquent-relationships": "^2.2",
        "calebporzio/sushi": "^2.5",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^3.5",
        "lorisleiva/laravel-actions": "^2.9",
        "rappasoft/laravel-livewire-tables": "^3.7.0",
        "spatie/laravel-navigation": "^1.3",
        "torann/geoip": "^3.0",
        "wire-elements/modal": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "barryvdh/laravel-ide-helper": "^3.5",
        "calebdw/larastan": "^3.2",
        "driftingly/rector-laravel": "^2.0",
        "fakerphp/faker": "^1.23",
        "filp/whoops": "^2.14",
        "laravel/breeze": "^2.3",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "peckphp/peck": "^0.1.3",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-faker": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.1",
        "pestphp/pest-plugin-livewire": "^3.0",
        "phpstan/extension-installer": "^1.4",
        "povils/phpmnd": "^3.4",
        "rector/rector": "^2.0",
        "spatie/laravel-ignition": "^2.9",
        "spatie/pest-plugin-test-time": "^2.0",
        "wire-elements/wire-spy": "^0.0.9"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": false,
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "analyze": "./vendor/bin/phpstan analyze --memory-limit=2G app",
        "type-coverage": "php artisan test --type-coverage",
        "test": "php artisan test",
        "test-coverage": "php artisan test --coverage",
        "format": "vendor/bin/pint",
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    }
}
