{
    "name": "jeffreydavidson/ringside",
    "type": "project",
    "description": "A project to help wrestling promoters run their companies.",
    "keywords": [
        "laravel",
        "wrestling"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "fidum/laravel-eloquent-morph-to-one": "^2.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.17",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.10",
        "lorisleiva/laravel-actions": "^2.4",
        "spatie/pest-plugin-test-time": "^1.1",
        "staudenmeir/laravel-merged-relations": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "doctrine/dbal": "^3.3",
        "fakerphp/faker": "^1.19",
        "filp/whoops": "^2.14",
        "laravel/breeze": "^1.9",
        "laravel/pint": "^1.0.0",
        "laravel/sail": "^1.14",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.1",
        "nunomaduro/phpinsights": "^2.4",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-faker": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.2",
        "worksome/request-factories": "^1.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": false,
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "pestphp/pest-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "php-cs-fixer": "php-cs-fixer --config=./.php-cs-fixer.php fix",
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    }
}
